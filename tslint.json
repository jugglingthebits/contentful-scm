{
    "extends": "tslint:recommended",
    "rules": {
        "interface-name": [true, "never-prefix"],
        "curly": [true, "as-needed"],
        "max-line-length": [true, 160],
        "no-angle-bracket-type-assertion": false,
        "arrow-parens": false,
        "switch-default": true
    }
}